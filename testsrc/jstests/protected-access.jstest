// Rhino must access a protected method of AbstractAction

var obj = new javax.swing.AbstractAction() {

  fireNameChange: function(oldValue, newValue) {
    this.firePropertyChange('name', oldValue, newValue);
  }

};

obj.fireNameChange(null, 'foo');

"success";
